<% if logged_in? %>
  <%= render :partial => "beers/menu", locals: {user: current_user} %>
<% else%>
  <%= link_to "Signup or Login", '/' %>
  <% end %>
<table>
    <tr>
      <th>User</th>
      <th># of Beers Tapped</th>
    </tr>
<% @users.each do |user| %>
    <tr>
      <td><%= link_to user.name, user_path(user) %></td>
      <td><%= user.beers.count %></td>
    </tr>
<% end %>
</table>
