<%= link_to "Back", '/'%>
<%= form_for(@user) do |f|%>
<% if @user.errors.any? %>
  <ul>
    <% @user.errors.full_messages.each do |message| %>
      <li><%= message %></li>
      <% end %>
    </ul>
<% end %>
<p>Username: <%= f.text_field :name%></p>
<p>Password: <%= f.password_field :password%> </p>
<p>Email: <%= f.text_field :email%></p>
<%= f.submit "Create New User"%>
<% end %>
